<?xml version='1.0' encoding='utf-8'?>
<component
    name='ml_RowSettings'
    extends='ContentNode'>
  <interface>
    <field
        id='componentName'
        type='string' />
    <field
        id='headerSettings'
        type='node' />
    <field
        id='focusSettings'
        type='node' />
    <field
        id='cellSettings'
        type='node' />
    <field
        id='contentOffset'
        type='floatarray'
        value='[0,0]' />
    <field
        id='space'
        type='integer'
        value='0' />
    <field
        id='screenPos'
        type='integer'
        value='-1' />
    <!-- pixels of the following row to show - will set the screen pos to ensure that thix quantity of pixels are available -->
    <field
        id='peekNextPixels'
        type='integer'
        value='0' />
    <field
        id='height'
        type='integer'
        value='-1' />
    <field
        id='isHiddenWhenEmpty'
        type='boolean' />
    <field
        id='propagateScroll'
        type='boolean'
        value='false' />
    <field
        id='propagateScrollUpdate'
        type='boolean'
        value='false' />
    <field
        id='propagateEnterExit'
        type='boolean'
        value='true' />
    <field
        id='numberItemsToRenderBefore'
        type='integer'
        value='4' />
    <field
        id='numberItemsToRenderAfter'
        type='integer'
        value='4' />
    <field
        id='renderStrategy'
        type='string'
        value='dynamic' />
    <!-- value='all' /> -->
  </interface>
</component>