<?xml version='1.0' encoding='utf-8'?>
<component
    name='ml_ListSettings'
    extends='ContentNode'>
  <interface>
    <field
        id='animSpeeds'
        type='array'
        value='[0.3, 0.2, 0.1]' />
    <field
        id='fastAnimateToEndEnabled'
        type='boolean'
        value='true' />
    <field
        id='allowLongPress'
        type='boolean'
        value='true' />
    <field
        id='sendScrollStartStop'
        type='boolean'
        value='false' />
    <field
        id='sendScrollUpdate'
        type='boolean'
        value='false' />

    <!-- async rendering support -->
    <!-- all|async -->
    <field
        id='rowCreationStrategy'
        type='string'
        value='all' />
    <field
        id='rowCreationBatchSize'
        type='integer'
        value='3' />
    <field
        id='rowCreationBatchDelay'
        type='float'
        value='0.15' />
    <field
        id='numberItemsToRenderBefore'
        type='integer'
        value='4' />
    <field
        id='numberItemsToRenderAfter'
        type='integer'
        value='4' />
    <field
        id='renderStrategy'
        type='string'
        value='dynamic' />

    <!-- index|content-->
    <field
        id='postUpdateIndexMode'
        type='string'
        value='index' />
  </interface>
</component>