      m.list.focusIndicatorManager = focusIndicatorManager
      m.expectCalled(m.list.focusIndicatorManager@.setFocusedOnRow(true))
      m.list.onGainedFocus(true)
      m.assertFalse(m.list.focusIndicatorManager.isHidden)
