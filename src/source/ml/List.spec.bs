    @describe("onUpdateNow")
    '+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

    @it("force list to update")
    function _()
      m.expectCalled(m.list.forceUpdate(true))

      m.list.onUpdateNow()
