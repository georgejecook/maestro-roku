    @params("options")
    @params("back")
    function _(key)
      m.expectNotCalled(m.list.itemScroller.cancelScrolling)
      m.expectNotCalled(m.list.getFocusedRow)

      m.assertFalse(m.list.onLongPressFinish(key))
